Public Class GetProvinceAbbreviation
    Private Shared _ProvinceDictionary As Dictionary(Of String, String)
    Public Shared Function Fetch(provinceName As String) As String
        If _ProvinceDictionary Is Nothing Then _ProvinceDictionary = FetchDictionary()
        Dim result As String = Nothing
        If Not _ProvinceDictionary.TryGetValue(provinceName, result) Then result = "NA"
        If provinceName.Length = 2 Then result = provinceName
        Return result
    End Function
